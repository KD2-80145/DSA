package com.sunbeam;

public class Solution1Main {

	public static int linearSearchComparisons(int arr[], int key) {
		
		int comparisons = 0;
		for (int i = 0; i < arr.length; i++) {
			comparisons++;
			if (arr[i] == key) {
				return comparisons;
			}
		}

		return arr.length;
	}

	public static void main(String[] args) {

	}

}
