package com.sunbeam;

public class Solution1Main {

	public static int linearSearchComparisons(int arr[], int key) {
		
		int comparisons = 0;
		for (int i = 0; i < arr.length; i++) {
			if (arr[i] == key)
				comparisons++;
		}

		return comparisons;
	}

	public static void main(String[] args) {

	}

}
